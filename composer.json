{
    "name": "meinder-a/laravel-forum",
    "description": "A forum package for Laravel applications",
    "keywords": ["laravel", "forum", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "meinder-a",
            "email": "me@meinder-a.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MeinderA\\LaravelForum\\": "/src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^0.3.8"
    }
}
