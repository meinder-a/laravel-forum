{
    "name": "meinder-a/laravel-forum",
    "description": "A forum package for Laravel applications",
    "keywords": ["laravel", "forum", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "meinder-a",
            "email": "me@meinder-a.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "MeinderA\\LaravelForum\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeinderA\\LaravelForum\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MeinderA\\LaravelForum\\ForumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
